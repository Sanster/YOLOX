{
  "pack": {
    "type": "tensor_from_image",
    "image": "image",
    "transforms": [
      {
        "type": "image_to_image",
        "name": "scale",
        "width": "$scaleWidth",
        "height": "$scaleHeight"
      },
      {
        "type": "image_to_tensor",
        "name": "rgb_norm",
        "mean": [0.0, 0.0, 0.0],
        "std": [0.0039216, 0.0039216, 0.0039216]
      }
    ]
  },
  "unpack": {
    "type": "bounding_boxes",
    "key": "boundingBoxes",
    "dtype": "float",
    "probabilityThreshold": "$probabilityThreshold",
    "classes": ["dog", "person", "dog bed"]
  }
}
